<?xml version="1.0" encoding="UTF-8"?>
 <configuration
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="logback.xsd">
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %5p | %-55logger{55} | %m %n</pattern>
		</encoder>
	</appender>
	<logger name="com.suretouch">
		<level value="INFO" />
	</logger>
	
	
	<logger name="org.dozer.DozerBeanMapper">
		<level value="WARN" />
	</logger>	
	
	<logger name="org.springframework">
		<level value="INFO" />
	</logger>
	
	
	<logger name="c.s.c.h.provider">
		<level value="WARN" />
	</logger>
	
	
	<logger name="o.s.w.s.m.m.annotation.RequestMappingHandlerMapping">
		<level value="WARN" />
	</logger>
	
	 
	
	<logger name="o.h.e.internal.StatisticalLoggingSessionEventListener">
		<level value="WARN" />
	</logger>
	<root>
		<level value="INFO" />
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>